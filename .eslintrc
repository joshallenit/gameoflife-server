{
  "rules": {
    "block-scoped-var": 0,
    "brace-style": ["2", "stroustrup"],
    "consistent-this": [2, "self"],
    "no-unused-expressions": 1,
    "no-unused-vars": [1, "local"],
    "no-use-before-define": [2, "nofunc"],
    "no-warning-comments": 2,
    "quotes": [0, "single"],
    "radix": 2,
    "semi": 2,
    "sort-vars": 0,
    "space-after-keywords": [0, "always"],
    "space-in-brackets": 0,
    "strict": 0,
    "use-isnan": 2,
    "valid-jsdoc": [0, {
      "requireReturn": false
    }],
    "valid-typeof": 2,
    "no-underscore-dangle": 0,
    "wrap-iife": 0,
    "wrap-regex": 0,
    "eqeqeq": 0,
    "key-spacing": 0,
    "no-multi-spaces" : 0,
    "no-undef": 0,
    "no-warning-comments": 1,
    "no-use-before-define": [0, "nofunc"],
    "space-infix-ops" : 1,
    "no-comma-dangle" :1,
    "eol-last":0,
    "no-shadow": 1,
    "camelcase": 0,
    "no-trailing-spaces": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
